{
  "title": "Alt+1 to toggle WezTerm with previous app",
  "rules": [
    {
      "description": "ctrl+0 toggles WezTerm and restores previous app",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "0",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open -a WezTerm"
            }
          ]
        }
      ]
    }
  ]
}
